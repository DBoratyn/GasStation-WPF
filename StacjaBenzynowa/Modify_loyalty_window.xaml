            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label13_Copy3" Content="1. Punkty:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="White" FontFamily="Copperplate Gothic Bold" FontSize="18" Width="410"/>
