        <Label x:Name="label13_Copy8" Content="Benzyna e98" HorizontalAlignment="Left" Margin="35,73,0,0" VerticalAlignment="Top" Foreground="White" FontFamily="Copperplate Gothic Light" FontSize="14" Width="105" Height="23"/>
        <Label x:Name="label13_Copy9" Content="Olej napędowy" HorizontalAlignment="Left" Margin="35,101,0,0" VerticalAlignment="Top" Foreground="White" FontFamily="Copperplate Gothic Light" FontSize="14" Width="125" Height="23"/>
        <Label x:Name="label13_Copy10" Content="LPG" HorizontalAlignment="Left" Margin="35,129,0,0" VerticalAlignment="Top" Foreground="White" FontFamily="Copperplate Gothic Light" FontSize="14" Width="45" Height="23"/>
