            Margin="10,317,0,0" VerticalAlignment="Top" Width="60" Background="{x:Null}" BorderBrush="{x:Null}" 
            Foreground="White" FontFamily="Copperplate Gothic Bold" FontSize="16" FontWeight="Bold" Click="Back_button_Click"/>
        <Button x:Name="Save_data_button"  Content="Zapisz zmiany" HorizontalAlignment="Left" 
            Margin="638,317,0,0" VerticalAlignment="Top" Width="144" Background="{x:Null}" BorderBrush="{x:Null}" 
            Foreground="White" FontFamily="Copperplate Gothic Bold" FontSize="16" FontWeight="Bold"/>
        <TextBox x:Name="benzyna_e95" HorizontalAlignment="Left" Height="23" Margin="145,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="35" Background="{x:Null}" BorderBrush="White" Foreground="White"/>
