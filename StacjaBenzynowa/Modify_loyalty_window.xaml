        <TextBox x:Name="mycie_standardowe" HorizontalAlignment="Left" Height="23" Margin="590,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="35" Background="{x:Null}" BorderBrush="White" Foreground="White"/>
