        <Label x:Name="label13_Copy27" Content="pkt" HorizontalAlignment="Left" Margin="290,276,0,0" VerticalAlignment="Top" Foreground="White" FontFamily="Copperplate Gothic Light" FontSize="14" Width="50" Height="23"/>

